<?xml version='1.0' encoding='utf-8'?>
<widget id="com.carismatic.carwash" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PushNotification">
        <param name="android-package" value="com.adobe.phonegap.push.PushPlugin" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/drawable-ldpi/icon.png" />
    <icon density="mdpi" src="res/drawable-mdpi/icon.png" />
    <icon density="hdpi" src="res/drawable-hdpi/icon.png" />
    <icon density="xhdpi" src="res/drawable-xhdpi/icon.png" />
    <name>Carismatic</name>
    <description>
        An application that connects car owners to car washers.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="android-build-tool" value="gradle" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
